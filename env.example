########################################
#  Core API Keys
########################################
# Weights & Biases API Key
WANDB_API_KEY=your_wandb_api_key_here

# OpenAI API Key (if using OpenAI models)
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API Key (if using Claude models)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Hugging Face Hub Access Token
HUGGINGFACE_HUB_TOKEN=your_huggingface_hub_token_here

# Mistral AI API Key
MISTRAL_API_KEY=your_mistral_api_key_here

# Cohere API Key
COHERE_API_KEY=your_cohere_api_key_here

# Google API Key (if using Google models)
GOOGLE_API_KEY=your_google_api_key_here

# Ling API Key (if using Ling models)
LING_API_KEY=your_ling_api_key_here

# Azure API Key (if using Azure‑hosted models)
AZURE_API_KEY=your_azure_api_key_here


########################################
#  AWS Credentials
########################################
AWS_ACCESS_KEY_ID=your_aws_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here
# Default region (e.g., us-west-2, ap-northeast-1)
AWS_DEFAULT_REGION=your_default_aws_region_here


########################################
#  LLM / Inference Server Settings
########################################
# vLLM API Key (set to EMPTY if not used)
VLLM_API_KEY=your_vllm_api_key_here_or_EMPTY

# Example for additional OpenAI‑compatible endpoints
# OPENAI_COMPATIBLE_API_KEY=your_other_openai_compatible_api_key_here


########################################
#  Docker Hub Credentials
########################################
DOCKER_USERNAME=your_dockerhub_username_here
DOCKER_PASSWORD=your_dockerhub_password_here

# vLLM Model Name (for vLLM container)
VLLM_MODEL_NAME=meta-llama/Meta-Llama-3-8B-Instruct

# Evaluation Config Path
EVAL_CONFIG_PATH=config-Meta-Llama-3-8B-Instruct.yaml 
